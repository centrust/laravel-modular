{
    "name": "idel/modular",
    "description": "Laravel modules",
    "keywords": [
        "laravel",
        "module",
        "modules",
        "modular",
        "laravel-modular",
        "idel",
        "laravel-modules"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Omid Eslami",
            "email": "oeslami32@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Idel\\Modular\\": "src/"
        },
        "files": [
            "./helper.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Idel\\Modular\\ServiceProvider"
            ],
            "aliases": {
                "Module": "Idel\\Modular\\Facades\\Module"
            }
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
